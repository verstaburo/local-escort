include ../container/container
include ../model-preview/model-preview
include ../grid/grid
include ../side-border-btn/side-border-btn
include ../badges/badges
include ../heading/heading
include ../image-bg/image-bg
include ../rating/rating
include ../short-info/short-info
include ../boost/boost
include ../vip/vip

mixin agency()
    +b.agency&attributes(attributes)
        +container
            +e.header
                +image-bg('agency/header.jpg')
                +e.row._info
                    +avatar('agency.png')._size_big
                    +badges._top
                        +badges-item._vip._skew
                            span Top 1
                    +e.col._heading
                        +e.info
                            +heading._md._uppercase Premium Escape
                            +e.UL.description
                                li Models 4057
                                li Since nov 16, 2012
                    +e.col._action
                        +short-info
                            +short-info-heading Information
                            +short-info-content
                                +short-info-text
                                    | About agency
                                    != pugHelpers.Icon.call(locals, {iconName: 'plus', className: 'agency__plus-icon', iconWidth: 12, iconHeight: 12})
                    +e.col._rating
                        +e.rating-label Rating
                        +rating(4)

                +e.row._about
                    +heading About agency
                    +e.P.text Mon nom est Megan, cultivée, je suis la plus belle compagne aventureuse pour n’importe quelle occasion. im the perfect woman that can give you the satisfaction, if you need relax and massage, call me, im real naughty, clean and safety so my picture is recent and 100% real, i propose Full package series kiseessss call me baby

                +e.row._short
                    +e.col
                        +short-info
                            +short-info-heading location
                            +short-info-content
                                +short-info-text United Kingdom, London
                    +e.col._phone
                        +short-info
                            +short-info-heading Phone
                            +short-info-content
                                +short-info-text +44 3069—990852
                    +e.col._site
                        +short-info
                            +short-info-heading Website
                            +short-info-content
                                +short-info-text Visit Website
                    +e.col._email
                        +short-info
                            +short-info-heading Email
                            +short-info-content
                                +short-info-text Send Email
                    +e.col._modified
                        span Modified: 15/01/2017
                        span Last Seen: 23/01/2017

            +e.models
                +heading Agency models
                +grid-masonry({ desktop: 4, laptop: 3, tablet: 2, mobile: 1}).js-model-preview-grid
                    +boost(true).js-masonry-item
                    +model-preview([7, 7, 7], 'Sonya', 20, 325, true).js-masonry-item
                        +badges._top
                            +badges-item._new._skew
                                span New
                            +badges-item._info
                                span 1 hour ago
                    +model-preview([5, 5, 5, 5], 'Megan', 22, 390).js-masonry-item
                        +badges._top
                            +badges-item._vip._skew
                                span Vip
                        +badges._bottom
                            +badges-item._verified._skew
                                span Verified
                    +model-preview([8, 8, 8, 8], 'Valeria', 26, 420).js-masonry-item
                        +badges._top
                            +badges-item._vip._skew
                                span Vip
                    +model-preview([9, 9, 9, 9, 9, 9, 9], 'Sonya', 20, 325).js-masonry-item
                        +badges._bottom._skew
                            +badges-item._verified._skew
                                span Verified
                    +model-preview([2, 2, 2], 'Christine', 27, 370, true).js-masonry-item
                        +badges._top
                            +badges-item._vip._skew
                                span Vip
                    +model-preview([6, 6, 6, 6], 'Anita', 24, 350).js-masonry-item
                        +badges._top
                            +badges-item._new._skew
                                span New
                            +badges-item._info._skew
                                span 1 hour ago
                            +badges-item._vip._skew
                                span Vip
                    +model-preview([1, 1, 1, 1, 1, 1], 'Kate', 25, 380).js-masonry-item
                        +badges._top
                            +badges-item._vip._skew
                                span Vip
                    +model-preview([10, 10], 'Anna', 22, 390).js-masonry-item
                    +vip(true).js-masonry-item
                    +model-preview([3, 3, 3, 3, 3], 'Sofi', 20, 325).js-masonry-item
                    +model-preview([4, 4, 4, 4, 4], 'Anna', 22, 390).js-masonry-item

                +side-border-btn('button').__load.js-load-models Load more
